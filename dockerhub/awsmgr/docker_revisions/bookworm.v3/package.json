{
  "name": "awsmgr",
  "version": "0.0.1",
  "description": "AWS Manager UX",
  "main": "server/index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "format": "pretty-quick --no-restage",
    "format:staged": "pretty-quick --staged",
    "format:all": "prettier --write .",
    "format-check": "pretty-quick --check",
    "prepare": "[ ${SKIP_PREPARE:-0} -eq 1 ] && echo 'Skipping husky install' || husky install",
    "dev": "next dev",
    "build": "npm run lint && next build",
    "pack": "find ./dist/ -mindepth 1 ! -name '.gitkeep' -exec rm -rf {} + && npm pack && mv awsmgr-?.?.?.tgz ./dist/.",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "start-server": "node server/index.js"
  },
  "files": [
    ".next",
    ".nvmrc",
    "awsmgr-start.sh",
    "bin",
    "declarations.d.ts",
    "next-env.d.ts",
    "next.config.js",
    "public",
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "postcss.config.js",
    "server",
    "src",
    "tsconfig.json"
  ],
  "bin": {
    "awsmgr-start": "./awsmgr-start.sh"
  },
  "repository": "git+https://github.com/hagan/cy-ui-aws-manager.git",
  "author": "Hagan Franks / University of Arizona / Data Science Institute",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/hagan/cy-ui-aws-manager/issues"
  },
  "homepage": "https://github.com/hagan/cy-ui-aws-manager#readme",
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/nextjs": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vercel/ncc": "^0.38.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "next-cli": "^1.0.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "pretty-quick": "^4.0.0",
    "storybook": "^7.6.10",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.1.0",
    "node": "^21.6.1",
